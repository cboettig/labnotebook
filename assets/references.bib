journal = {The New phytologist},
keywords = {Animals,Climate,Ecosystem,Europe,Geography,Larix,Larix: parasitology,Moths,Moths: physiology,Picea,Picea: parasitology,Spectrum Analysis,Temperature,Time Factors},
month = jun,
number = {4},
pages = {929--41},
pmid = {19383093},
title = {{Three centuries of insect outbreaks across the European Alps.}},
