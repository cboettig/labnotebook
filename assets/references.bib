month = sep,
number = {7159},
pages = {213--7},
pmid = {17851524},
title = {{Spatial vegetation patterns and imminent desertification in Mediterranean arid ecosystems.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17851524},
volume = {449},
year = {2007}
}
