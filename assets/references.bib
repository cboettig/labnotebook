journal = {The American naturalist},
keywords = {Animals,Biological Evolution,Ecosystem,Host-Parasite Interactions,Insects,Insects: growth \& development,Insects: physiology,Larva,Larva: growth \& development,Models, Theoretical,Plants,Plants: growth \& development,Plants: parasitology,Pollination,Population Density,Seeds,Seeds: growth \& development,Seeds: parasitology},
month = dec,
number = {6},
pages = {780--94},
pmid = {19845459},
title = {{Eco-evolutionary dynamics of mutualists and exploiters.}},
