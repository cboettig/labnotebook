pages = {14--25},
pmid = {21996447},
title = {{Regime shifts and heterogeneous trends in malaria time series from Western Kenya Highlands.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3252560\&tool=pmcentrez\&rendertype=abstract},
