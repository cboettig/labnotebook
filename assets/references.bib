issn = {0169-5347},
journal = {Trends in ecology \& evolution},
keywords = {alternate stable states,and that take even,borrowed time,climate change,dynamics,flips,longer to reverse,or,regime shift,resilience,slow responses,surprises,that,thresholds,transient,transitions represent more typical,unfold in slow motion},
month = mar,
number = {3},
pages = {149--55},
pmid = {22995893},
title = {{Living dangerously on borrowed time during slow, unrecognized regime shifts.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22995893},
volume = {28},
year = {2013}
}
