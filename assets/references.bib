keywords = {Climate,Conservation of Natural Resources,Uncertainty,United States,Wetlands},
month = apr,
number = {17},
pages = {6484--9},
pmid = {22451914},
title = {{Optimal portfolio design to reduce climate-related conservation uncertainty in the Prairie Pothole Region.}},
